@model List<BlogListModel>

<div class="card my-4">
    <h5 class="card-header">Son Eklenen Bloglar</h5>
    <div class="card-body">
        @foreach (var item in Model)
        {
            <p>
                <a asp-controller="Home" asp-action="BlogDetails" asp-route-id="@item.Id">@item.Title</a>
            </p>
            <hr />
        }
    </div>
</div>